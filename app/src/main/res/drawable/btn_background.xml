<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
<item>
    <shape android:shape="rectangle">
        <stroke android:width="2dp" android:color="#f87334"/>
        <gradient android:startColor="#f87334"
            android:endColor="#f87334"
            android:angle="45" android:centerX="0%"
            android:centerColor="#d94033"/>
        <corners android:topLeftRadius="10dp" android:topRightRadius="10dp"
            android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp"/>
    </shape>
</item>
</selector>

